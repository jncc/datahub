{
  "name": "datahub-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "OGL",
  "private": true,
  "scripts": {
    "search:pull": "docker pull docker.elastic.co/elasticsearch/elasticsearch:6.4.2 && docker pull amazon/dynamodb-local",
    "search:run": "cd ./search/dev && docker run -p 9200:9200 -p 9300:9300 -e \"discovery.type=single-node\" $(docker build -q .)",
    "search:run:windows": "cd \"./search/dev\"; docker run -p 9200:9200 -p 9300:9300 -e \"discovery.type=single-node\" $(docker build -q .)",
    "db:run": "docker run -p 8200:8000 amazon/dynamodb-local",
    "search:setup": "cd ./search/setup && node setup.js",
    "deploy:initial": "claudia create --name jncc-datahub-live-backend --region eu-west-1 --api-module index --policies policy ",
    "deploy": "claudia update"
  },
  "dependencies": {
    "aws-sdk": "^2.343.0",
    "claudia-api-builder": "^4.1.1",
    "dotenv": "6.2.0",
    "request": "^2.88.0",
    "uuid": "^3.3.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "claudia": "^5.1.2",
    "glob": "^7.1.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  }
}
